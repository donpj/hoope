{
  "expo": {
    "name": "hoopeMvp",
    "slug": "hoopeMvp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.gigipiscitelli.hoopeMvp",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gigipiscitelli.hoopeMvp",
      "buildNumber": "6",
      "entitlements": {
        "aps-environment": "development",
        "com.apple.developer.associated-domains": [
          "applinks:revolut.com",
          "applinks:hoope.co"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gigipiscitelli.hoopeMvp",
      "versionCode": 2,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "revolut.com",
              "pathPrefix": "/app/edfedc4b-bd94-45a2-a3ce-3992b102c935"
            },
            {
              "scheme": "https",
              "host": "hoope.co",
              "pathPrefix": "/revolut-redirect"
            },
            {
              "scheme": "com.gigipiscitelli.hoopeMvp"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router", { "origin": "https://hoope.co" }],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "apiUrl": "http://localhost:8081",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e027d1d4-f843-4a35-9f2c-5315b4e35251"
      }
    },
    "owner": "gigihoope"
  }
}
